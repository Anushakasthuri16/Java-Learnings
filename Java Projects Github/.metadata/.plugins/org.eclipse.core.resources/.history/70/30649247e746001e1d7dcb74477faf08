package _35_Amstrong_Number;

public class Amstrong {
	int amsnum(int n)
	{
		
		while(n>0)
		{
			int finalresult=1;
			int sum=0;
			
			for (int i=1;i<n;i++)
			{   
				
				
				int ld=n%10;
				finalresult=finalresult*n;
				sum=sum+finalresult;
				n=n/10;
				
			}
			
		}
		;
		
	
	}
	

	

}
