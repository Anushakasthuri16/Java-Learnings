package com.example.spring.SpringProject3ListSetMap;

public class CompanyXYZ {
	int empId;
	String empName;
	int empSalary;
	public CompanyXYZ(int empId, String empName, int empSalary) {
		super();
		this.empId = empId;
		this.empName = empName;
		this.empSalary = empSalary;
	}
	public CompanyXYZ() {
		super();
		// TODO Auto-generated constructor stub
	}
	public int getEmpId() {
		return empId;
	}
	public void setEmpId(int empId) {
		this.empId = empId;
	}
	public String getEmpName() {
		return empName;
	}
	public void setEmpName(String empName) {
		this.empName = empName;
	}
	public int getEmpSalary() {
		return empSalary;
	}
	public void setEmpSalary(int empSalary) {
		this.empSalary = empSalary;
	}
	@Override
	public String toString() {
		return "CompanyXYZ [empId=" + empId + ", empName=" + empName + ", empSalary=" + empSalary + "]";
	}
	
	

}
