<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">

<!-- setter injection -->
<bean class="com.example.spring.SpringProject3ListSetMap.Employee" id="emp">
	<property name="technology">
		<list>
			<value>Java</value>
			<value>Python</value>
			<value>SQL</value>
		</list>
	</property>
	<property name="project">
		<set>
			<value>Project1</value>
			<value>Project2</value>
			<value>Project3</value>
		</set>
	</property>
	<property name="company">
		<map>
			<entry key="121" value="XYZ"></entry>
			<entry key="123" value="PQR"></entry>
		</map>
	</property>
</bean>
</beans>