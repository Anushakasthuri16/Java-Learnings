<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">

<!-- setter injection -->
<bean id="lt1" class="com.example.spring.SpringProject2Laptop.Laptop">
<property name="sNo" value="1"></property>
<property name="brand" value="Hp"></property>
<property name="cost" value="60000.56f"></property>
</bean>

<!--  Constructor injection -->
<bean id="lt2" class="com.example.spring.SpringProject2Laptop.Laptop">
<constructor-arg value="2"></constructor-arg>
<constructor-arg value="Dell"></constructor-arg>
<constructor-arg value="55000.65f"></constructor-arg>
</bean>
</beans>