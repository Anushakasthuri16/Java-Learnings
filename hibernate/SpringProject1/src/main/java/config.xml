<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">


<bean class="com.example.spring.SpringProject1.Student" id="st">
	<property name="roll" value="13"></property>
	<property name="name" value="sumit"></property>
	<property name="age" value="22"></property>
</bean>
<bean class="com.example.spring.SpringProject1.JavaTeacher" id="jvt">
	<property name="teacher_id" value="23"></property>
	<property name="teacher_Name" value="Anu"></property>

</bean>
<bean class="com.example.spring.SpringProject1.SqlTeacher" id="sqt">
	<property name="teacher_id" value="14"></property>
	<property name="teacher_Name" value="Anusha"></property>
	
</bean>
<bean class="com.example.spring.SpringProject1.PythonTeacher" id="pyt">
	<property name="teacher_id" value="15"></property>
	<property name="teacher_Name" value="Kasthuri"></property>

</bean>
</beans>